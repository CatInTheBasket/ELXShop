
<html>
    <body>
        <table>
            <tr>
                <th>ID</th>
                <th>Title</th>
                <th>Category</th>
                <th>Price</th>
                <th>Stock</th>
                <th>Image</th>                
            </tr>
            
            <% if(result){ %>
                <tr>
                    <td><%= result.id %></td>
                    <td><%= result.title %></td>
                    <td><%= result.price %></td>
                    <td><%= result.stock %></td>
                    <td><%= result.category %></td>
                    <td><img src="<%= '/img/'+result.imageURL %>"></img></td>
                </tr>
            <% } %>
        </table>
    </body>
</html>